package com.comcast.homesecurity.domain.dao;

import static org.junit.Assert.*;

import org.junit.Test;

public class EventDaoTest {
	  
	@Test
	public void formatEventTimeStamp_FormatsInGMT24HrTime() {
		EventDao eventDao = new EventDao(null);
		long ts1 = 1403024280771L;
		long ts2 = 1403236569000L;
		String timestampStr1 = eventDao.formatEventTimeStamp(ts1);
		assertEquals("20140617_165800", timestampStr1);
		String timestampStr2 = eventDao.formatEventTimeStamp(ts2);
		assertEquals("20140620_035609", timestampStr2);
	}

}
